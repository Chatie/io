{
  "name": "@chatie/io",
  "version": "0.2.2",
  "description": "Cloud Input/Output Service for Chatie",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "dependencies": {
    "brolog": "^1.8.1",
    "listag": "^0.8.3",
    "moment": "^2.22.2",
    "read-pkg-up": "^4.0.0",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "@types/blue-tape": "^0.1.32",
    "@types/node": "^10.5.5",
    "@types/read-pkg-up": "^3.0.1",
    "@types/semver": "^5.5.0",
    "@types/sinon": "^5.0.1",
    "@types/ws": "^5.1.2",
    "blue-tape": "^1.0.0",
    "git-scripts": "^0.2.1",
    "markdownlint-cli": "^0.13.0",
    "semver": "^5.5.0",
    "shx": "^0.3.2",
    "sinon": "^6.1.4",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^7.1.0",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "clean": "shx rm -fr dist/*",
    "dist": "npm run clean && tsc",
    "pack": "npm pack",
    "example:server": "ts-node examples/server.ts",
    "lint": "npm run lint:ts && npm run lint:md",
    "lint:md": "markdownlint README.md",
    "lint:ts": "tslint --project tsconfig.json && tsc --noEmit",
    "test": "npm run lint && npm run test:unit",
    "test:pack": "bash -x scripts/npm-pack-testing.sh",
    "test:unit": "blue-tape -r ts-node/register \"src/**/*.spec.ts\" \"src/*.spec.ts\" \"tests/*.spec.ts\" \"tests/**/*.spec.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chatie/io.git"
  },
  "keywords": [
    "chatie",
    "io",
    "cloud",
    "protocol"
  ],
  "author": "Huan LI <zixia@zixia.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Chatie/io/issues"
  },
  "git": {
    "scripts": {
      "pre-push": "./scripts/pre-push.sh"
    }
  },
  "homepage": "https://github.com/Chatie/io#readme"
